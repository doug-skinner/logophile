{
    "extends": ["prettier", "eslint:recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error"],
        "import/no-anonymous-default-export": "off"
    },
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "env": {
        "node": true,
        "es6": true
    }
}